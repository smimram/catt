coh unitl
    (x : *)
    (y : *) (f : x -> y)
    :
    comp id f -> f
let unitl f = unitl _ _ f

coh unitr
    (x : *)
    (y : *) (f : x -> y)
    :
    comp f id -> f
let unitr f = unitr _ _ f

