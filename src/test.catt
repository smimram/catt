coh id2
    (x : *)
    (y : *) (f : x -> y)
    :
    (f -> f)
let id2 (f : _ -> _) = id2 _ _ f

coh vcomp
    (x : *)
    (y : *) (f : x -> y)
    (g : x -> y) (a : f -> g)
    (h : x -> y) (b : g -> h)
    :
    f -> h
let vcomp (a : _ -> _) (b : _ -> _) = vcomp _ _ _ _ a _ b

coh hcomp
    (x : *)
    (y : *) (f : x -> y)
    (f' : x -> y) (a : f -> f')
    (z : *) (g : y -> z)
    (g' : y -> z) (b : g -> g')
    :
    comp f g -> comp f' g'
let hcomp (a : _ -> _) (b : _ -> _) = hcomp _ _ _ _ a _ _ _ b

coh ichg
    (x : *)
    (y : *) (f : x -> y)
    (g : x -> y) (a : f -> g)
    (h : x -> y) (b : g -> h)
    (z : *) (l : y -> z)
    (m : y -> z) (c : l -> m)
    (n : y -> z) (d : m -> n)
    :
    hcomp (vcomp a b) (vcomp c d) -> vcomp (hcomp a c) (hcomp b d)

