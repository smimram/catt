coh comp
    (x : *)
    (y : *) (f : x -> y)
    (z : *) (g : y -> z)
    :
    (x -> z)
let comp f g = comp _ _ f _ g

coh assoc
    (x : *)
    (y : *) (f : x -> y)
    (z : *) (g : y -> z)
    (w : *) (h : z -> w)
    :
    comp (comp f g) h -> comp f (comp g h)
let assoc f g h = assoc _ _ f _ g _ h

