coh assoc
    (x : *)
    (y : *) (f : x -> y)
    (z : *) (g : y -> z)
    (w : *) (h : z -> w)
    :
    comp (comp f g) h -> comp f (comp g h)
let assoc (f : _ -> _) (g : _-> _) (h : _ -> _) = assoc _ _ f _ g _ h

coh id (x : *) : (x -> x)
let id = id _

coh unitl
    (x : *)
    (y : *) (f : x -> y)
    :
    comp id f -> f
let unitl (f : _ -> _) = unitl _ _ f

coh unitr
    (x : *)
    (y : *) (f : x -> y)
    :
    comp f id -> f
let unitr (f : _ -> _) = unitr _ _ f

